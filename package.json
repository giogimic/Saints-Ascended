{
  "name": "saints-ascended",
  "version": "0.1.0",
  "description": "AI-First Ark: Survival Ascended Dedicated Server Manager UI",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:with-db": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:curseforge": "node scripts/test-curseforge.js",
    "verify:api": "node scripts/verify-api-key.js",
    "type-check": "tsc --noEmit",
    "verify": "node scripts/verify-installation.js",
    "setup": "node scripts/setup-env.js",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "npx cross-env DATABASE_URL=\"file:./prisma/data/mods.db\" prisma db push",
    "db:migrate": "npx cross-env DATABASE_URL=\"file:./prisma/data/mods.db\" prisma migrate dev",
    "db:deploy": "npx cross-env DATABASE_URL=\"file:./prisma/data/mods.db\" prisma migrate deploy",
    "db:deploy:direct": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset"
  },
  "dependencies": {
    "@headlessui/react": "2.2.0",
    "@heroicons/react": "2.2.0",
    "@hookform/resolvers": "3.10.0",
    "@monaco-editor/react": "^4.7.0",
    "@prisma/client": "^6.10.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "autoprefixer": "10.4.20",
    "chart.js": "4.4.7",
    "clsx": "^2.1.1",
    "daisyui": "^5.0.43",
    "date-fns": "4.1.0",
    "framer-motion": "11.15.0",
    "monaco-editor": "^0.52.2",
    "next": "15.1.3",
    "postcss": "8.5.2",
    "prisma": "^6.10.1",
    "react": "^18.3.1",
    "react-chartjs-2": "5.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "7.54.0",
    "react-hot-toast": "2.4.1",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "3.4.17",
    "uuid": "9.0.1",
    "ws": "8.18.2",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "1.51.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/node": "22.10.2",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@types/uuid": "9.0.8",
    "@types/ws": "8.5.13",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "4.3.4",
    "dotenv": "^16.5.0",
    "eslint": "8.57.1",
    "eslint-config-next": "15.1.3",
    "jsdom": "25.0.1",
    "playwright": "1.51.0",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.9",
    "typescript": "5.6.3",
    "vitest": "2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "ark",
    "survival",
    "ascended",
    "server",
    "manager",
    "nextjs",
    "tailwindcss",
    "ai-first"
  ],
  "author": "AI Developer",
  "license": "MIT",
  "trustedDependencies": [
    "esbuild",
    "sharp",
    "unrs-resolver"
  ]
}
