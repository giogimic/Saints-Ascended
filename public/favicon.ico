<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" fill="none">
  <rect width="32" height="32" fill="#0a0a0a"/>
  <rect x="2" y="2" width="28" height="28" fill="none" stroke="#00ff41" stroke-width="1"/>
  <rect x="6" y="6" width="20" height="20" fill="none" stroke="#00ff41" stroke-width="0.5" opacity="0.6"/>
  
  <!-- Server icon -->
  <rect x="10" y="8" width="12" height="3" fill="#00ff41" opacity="0.8"/>
  <rect x="10" y="12" width="12" height="3" fill="#00ff41" opacity="0.6"/>
  <rect x="10" y="16" width="12" height="3" fill="#00ff41" opacity="0.4"/>
  <rect x="10" y="20" width="12" height="3" fill="#00ff41" opacity="0.2"/>
  
  <!-- Power indicator -->
  <circle cx="24" cy="9" r="1" fill="#00ff41">
    <animate attributeName="opacity" values="0.3;1;0.3" dur="2s" repeatCount="indefinite"/>
  </circle>
  
  <!-- Matrix grid effect -->
  <defs>
    <pattern id="grid" width="4" height="4" patternUnits="userSpaceOnUse">
      <path d="M 4 0 L 0 0 0 4" fill="none" stroke="#00ff41" stroke-width="0.2" opacity="0.3"/>
    </pattern>
  </defs>
  <rect width="32" height="32" fill="url(#grid)"/>
</svg> 